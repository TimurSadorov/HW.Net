@model ResultModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<form method="post" asp-action="Calculate">
    <input type="text" placeholder="Введите выражение" name="expression"/>
    <input type="submit" value="Вычеслить"/>
</form>
<span id="response" class="mt-3">@Model?.Result</span>

<form method="post" asp-action="StartCalculateTenMillion">
    <button type="submit">Вычеслить 10млн выражений</button>
</form>